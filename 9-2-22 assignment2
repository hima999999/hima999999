#include <stdio.h>
void display(int);
int main()
{
  int limit;
  printf("enter the number of terms to be printed\n");
  scanf("%d",&limit);
  printf("\n natural numbers from 1 to %d are:",limit);
  display(limit);
  return 0;
}
void display(int num)
{
  if(num)
  display(num-1);
  else
  return;
  printf("\n%d\n",num);
  }
